{
  "framework": null,
  "builds": [
    {
      "src": "*.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    },
    {
      "src": "templates/**",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/app.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Set-Cookie",
          "value": "SameSite=Lax; Secure"
        }
      ]
    }
  ]
}
